{
  "name": "frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 4201",
    "start:prod": "ng serve --configuration network --host 0.0.0.0 --port 4200",
    "start:network": "ng serve --configuration network --host 0.0.0.0",
    "serve:prod": "serve -s dist/frontend/browser -l 4200 --no-port-switching --no-clipboard",
    "build": "ng build",
    "build:network": "ng build --configuration network",
    "build:prod": "ng build --configuration production",
    "watch": "ng build --watch --configuration development",
    "test": "ng test"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "packageManager": "npm@11.6.2",
  "dependencies": {
    "@angular/common": "^21.0.0",
    "@angular/compiler": "^21.0.0",
    "@angular/core": "^21.0.0",
    "@angular/forms": "^21.0.0",
    "@angular/platform-browser": "^21.0.0",
    "@angular/router": "^21.0.0",
    "chart.js": "^4.5.1",
    "html2canvas": "^1.4.1",
    "jspdf": "^3.0.4",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@angular/build": "^21.0.1",
    "@angular/cli": "^21.0.1",
    "@angular/compiler-cli": "^21.0.0",
    "http-server": "^14.1.1",
    "jsdom": "^27.1.0",
    "serve": "^14.2.4",
    "typescript": "~5.9.2",
    "vitest": "^4.0.8"
  }
}
